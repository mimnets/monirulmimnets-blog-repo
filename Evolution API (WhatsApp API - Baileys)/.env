# Authentication settings
AUTHENTICATION_API_KEY=replace_with_secure_api_key
CONFIG_SESSION_PHONE_CLIENT=EvolutionAPI

# Server settings
SERVER_URL=http://192.168.22.205:8080

# PostgreSQL settings
DATABASE_ENABLED=true
DATABASE_PROVIDER=postgresql
DATABASE_CONNECTION_URI=postgresql://postgres:replace_with_secure_password@postgres:5432/evolution
DATABASE_CONNECTION_CLIENT_NAME=evolution_api
DATABASE_SAVE_DATA_INSTANCE=true
DATABASE_SAVE_DATA_NEW_MESSAGE=true
DATABASE_SAVE_MESSAGE_UPDATE=true
DATABASE_SAVE_DATA_CONTACTS=true
DATABASE_SAVE_DATA_CHATS=true
DATABASE_SAVE_DATA_LABELS=true
DATABASE_SAVE_DATA_HISTORIC=true

# Redis settings
CACHE_REDIS_ENABLED=true
CACHE_REDIS_URI=redis://evolution_redis:6379/1
CACHE_REDIS_PREFIX_KEY=evolution_api
CACHE_REDIS_SAVE_INSTANCES=false
CACHE_LOCAL_ENABLED=false

# S3 settings (optional, leave empty if not using S3)
S3_ENABLED=false
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_BUCKET=
S3_PORT=443
S3_ENDPOINT=
S3_USE_SSL=true

# RabbitMQ settings (optional, disabled by default)
RABBITMQ_ENABLED=false
RABBITMQ_URI=amqp://admin:admin@rabbitmq:5672/default

# PostgreSQL credentials
POSTGRES_USER=postgres
POSTGRES_PASSWORD=replace_with_secure_password
POSTGRES_DB=evolution
